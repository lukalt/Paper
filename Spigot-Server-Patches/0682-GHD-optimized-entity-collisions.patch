+package com.destroystokyo.paper.util.misc;
+
+import java.util.concurrent.Callable;
+
+/**
+ * @author Lukas Alt
+ * @since 11.01.2021
+ */
+public abstract class CachingCallable<V> implements Callable<V> {
+    private V cachedValue;
+
+    @Override
+    public V call() {
+        if (cachedValue == null) {
+            cachedValue = call0();
+        }
+        return cachedValue;
+    }
+
+    protected abstract V call0();
+}
